@{
    ViewData["Title"] = "| Inscripicón";
}
<!doctype html>
<html lang="es">
  <head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0"
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="css/estilos.css">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,700,800&amp;display=swap" rel="stylesheet">

	<link href="https://fonts.googleapis.com/css?family=Lato:300,400,700&display=swap" rel="stylesheet">

	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
	<link rel="stylesheet" href="~/css/style.css">

	</head>
	<body>
		<header>
			<div class="atras"> 
			</div> 
		</header>
    
		<br>
    
		<main>

			<a class="nav-link text-black" asp-area="" asp-controller="Home" asp-action="SelecActividadGrupal">ATRAS</a>
			<a href="http://localhost:5240/Home/SelecActividadGrupal"><img src="~/lib/atras.png" width="30" height="20" alt="atras" class="flecha-atras"><a/>
			<section class="ftco-section">
				<div class="container" >
					<div class="row justify-content-center">
					<div class="col-md-6 text-center mb-5">
						<h2 class="heading-section"></h2>
					</div>
				</div>
				<div class="row justify-content-center" style="background-image: url('/lib/servicio1.jpg'); ">
					<div class="col-md-12 col-lg-10">
				
						<div class="login-wrap p-4 p-md-5">
			      			<div class="d-flex">		
								<div class="wrap d-md-flex">
									
								</div>	
			      				<div class="w-100">
			      					
			      				</div>
								<div class="w-100">
									
								</div>
			      			</div>
							<form action="http://localhost:5240/Home/PlataformaAdministrativa" class="signin-form">
			      			<div class="form-group mb-3">
			      					<label class="label" for="name">Nombres:</label>
			      					<input type="text" class="form-control" placeholder="Angel Espinoza Mendieta">
			      			</div>
							<div class="form-group mb-3">
			      					<label class="label" for="name">Actividad:</label>
			      					<input type="text" class="form-control" placeholder="Muay Thai">
			      			</div>
							  <div class="form-group mb-3">
			      					<label class="label" for="name">Fecha:</label>
			      					<input type="text" class="form-control" required>
			      			</div>
							<div class="form-group mb-3">
		            			<label class="label" for="password">Vacantes:</label>
							  <input type="text" class="form-control" placeholder="5">
							</div>
							<div class="form-group">
		            				<button type="submit" class="form-control btn btn-primary rounded submit px-3">Inscribirse</button>
							</div>
						  </form>
						</div>
					 </div>
				</div>
				 </div>
			</section>

		<script src="js/jquery.min.js"></script>
		<script src="js/popper.js"></script>
		<script src="js/bootstrap.min.js"></script>
		<script src="js/main.js"></script>
            
    </main>
		
		

	</body>
</html>